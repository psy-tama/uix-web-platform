{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["no-relative-import-paths", "@typescript-eslint"],
  "rules": {
    "no-console": "error",
    "@typescript-eslint/no-unused-vars": "error",
    "no-relative-import-paths/no-relative-import-paths": [
      "error",
      { "allowSameFolder": true }
    ]
  }
}
